{
    "manifest_version": 3,
    "name": "NoteWise",
    "description": "Base Level Extension",
    "version": "1.0.1",
    "permissions": ["storage", "tabs", "activeTab", "scripting"],
  
    "action" : 
    {
      "default_popup" : "popup.html"
    },

    "content_scripts": 
    [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}