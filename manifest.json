{
    "manifest_version": 3,
    "name": "NoteWise",
    "description": "Base Level Extension",
    "version": "1.0.1",
    "permissions": ["storage", "tabs"],
  
    "action" : 
    {
      "default_popup" : "popup.html"
    },

    "content_scripts": 
    [
      {
        "matches": ["*://google.com/*"],
        "js": ["content.js"]
      }
    ]
}